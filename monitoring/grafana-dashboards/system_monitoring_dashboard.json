{
  "dashboard": {
    "id": null,
    "title": "SVG-AI System Monitoring Dashboard",
    "tags": ["svg-ai", "monitoring", "analytics"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Performance",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(api_response_time)",
            "legendFormat": "Avg Response Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 0.2}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "API Requests per Second",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(api_requests_total[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "CPU Utilization",
        "type": "graph",
        "targets": [
          {
            "expr": "cpu_percent",
            "legendFormat": "CPU %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Memory Utilization",
        "type": "graph",
        "targets": [
          {
            "expr": "memory_percent",
            "legendFormat": "Memory %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Processing Queue Length",
        "type": "stat",
        "targets": [
          {
            "expr": "queue_length",
            "legendFormat": "Queue Length"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 20}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Conversion Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(successful_conversions[5m]) / (rate(successful_conversions[5m]) + rate(failed_conversions[5m]))",
            "legendFormat": "Success Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.95}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 8}
      },
      {
        "id": 7,
        "title": "Quality Improvement Trends",
        "type": "graph",
        "targets": [
          {
            "expr": "avg_over_time(quality_improvement[1h])",
            "legendFormat": "Quality Improvement"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 8,
        "title": "Method Effectiveness by Logo Type",
        "type": "heatmap",
        "targets": [
          {
            "expr": "method_effectiveness",
            "legendFormat": "{{method}}_{{logo_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 9,
        "title": "Resource Utilization Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "cpu_percent",
            "legendFormat": "CPU %"
          },
          {
            "expr": "memory_percent",
            "legendFormat": "Memory %"
          },
          {
            "expr": "gpu_utilization",
            "legendFormat": "GPU %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 10,
        "title": "Processing Time Analytics",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, avg_processing_time)",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, avg_processing_time)",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, avg_processing_time)",
            "legendFormat": "P99"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s",
    "schemaVersion": 16,
    "version": 1
  }
}