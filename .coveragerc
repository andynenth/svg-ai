[run]
source = backend/ai_modules
omit =
    */tests/*
    */test_*
    */__pycache__/*
    */migrations/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

[html]
directory = coverage_html_report
title = AI Modules Test Coverage Report

[xml]
output = coverage.xml