        "# Advanced Contrastive Learning for Logo Representation",
        "class LogoSimCLR(nn.Module):",
        "    \"\"\"SimCLR with logo-specific augmentations and projections\"\"\"",
        "    ",
        "    def __init__(self, base_encoder, projection_dim=256, temperature=0.07):",
        "        super().__init__()",
        "        ",
        "        self.encoder = base_encoder",
        "        self.temperature = temperature",
        "        ",
        "        # Get encoder output dimension",
        "        with torch.no_grad():",
        "            dummy_input = torch.randn(1, 3, 224, 224)",
        "            if hasattr(self.encoder, 'forward_features'):",
        "                encoder_dim = self.encoder.forward_features(dummy_input).shape[-1]",
        "            else:",
        "                encoder_dim = self.encoder(dummy_input)[0].shape[-1]  # For our ViT",
        "        ",
        "        # Logo-aware projection head",
        "        self.projection_head = nn.Sequential(",
        "            nn.Linear(encoder_dim, encoder_dim),",
        "            nn.BatchNorm1d(encoder_dim),",
        "            nn.ReLU(inplace=True),",
        "            nn.Linear(encoder_dim, encoder_dim // 2),",
        "            nn.BatchNorm1d(encoder_dim // 2),",
        "            nn.ReLU(inplace=True),",
        "            nn.Linear(encoder_dim // 2, projection_dim)",
        "        )",
        "        ",
        "        # Logo-specific feature extractors",
        "        self.geometric_projector = nn.Linear(encoder_dim, projection_dim // 4)",
        "        self.color_projector = nn.Linear(encoder_dim, projection_dim // 4)",
        "        self.text_projector = nn.Linear(encoder_dim, projection_dim // 4)",
        "        self.shape_projector = nn.Linear(encoder_dim, projection_dim // 4)",
        "        ",
        "    def forward(self, x1, x2):",
        "        # Extract features from both augmented views",
        "        if hasattr(self.encoder, 'forward_features'):",
        "            h1 = self.encoder.forward_features(x1)",
        "            h2 = self.encoder.forward_features(x2)",
        "            h1 = h1.mean(dim=1) if h1.dim() > 2 else h1",
        "            h2 = h2.mean(dim=1) if h2.dim() > 2 else h2",
        "        else:",
        "            h1, _, _ = self.encoder(x1)  # Our ViT returns (logits, uncertainty, prior)",
        "            h2, _, _ = self.encoder(x2)",
        "            # Extract features before classification head",
        "            h1 = self.encoder.to_latent(h1)",
        "            h2 = self.encoder.to_latent(h2)",
        "        ",
        "        # Main projections",
        "        z1 = F.normalize(self.projection_head(h1), dim=1)",
        "        z2 = F.normalize(self.projection_head(h2), dim=1)",
        "        ",
        "        # Logo-specific projections",
        "        z1_geo = F.normalize(self.geometric_projector(h1), dim=1)",
        "        z2_geo = F.normalize(self.geometric_projector(h2), dim=1)",
        "        ",
        "        z1_color = F.normalize(self.color_projector(h1), dim=1)",
        "        z2_color = F.normalize(self.color_projector(h2), dim=1)",
        "        ",
        "        z1_text = F.normalize(self.text_projector(h1), dim=1)",
        "        z2_text = F.normalize(self.text_projector(h2), dim=1)",
        "        ",
        "        z1_shape = F.normalize(self.shape_projector(h1), dim=1)",
        "        z2_shape = F.normalize(self.shape_projector(h2), dim=1)",
        "        ",
        "        return {",
        "            'main': (z1, z2),",
        "            'geometric': (z1_geo, z2_geo),",
        "            'color': (z1_color, z2_color),",
        "            'text': (z1_text, z2_text),",
        "            'shape': (z1_shape, z2_shape)",
        "        }",
        "    ",
        "    def contrastive_loss(self, projections, weights=None):",
        "        \"\"\"Multi-aspect contrastive loss for logo features\"\"\"",
        "        if weights is None:",
        "            weights = {'main': 1.0, 'geometric': 0.3, 'color': 0.3, 'text': 0.3, 'shape': 0.3}",
        "        ",
        "        total_loss = 0",
        "        loss_dict = {}",
        "        ",
        "        for aspect, (z1, z2) in projections.items():",
        "            # Combine views",
        "            z = torch.cat([z1, z2], dim=0)",
        "            batch_size = z1.shape[0]",
        "            ",
        "            # Compute similarity matrix",
        "            sim_matrix = torch.matmul(z, z.T) / self.temperature",
        "            ",
        "            # Create positive pairs mask",
        "            mask = torch.eye(2 * batch_size, dtype=torch.bool, device=z.device)",
        "            mask = mask.fill_diagonal_(False)",
        "            ",
        "            # Positive pairs: (i, i+batch_size) and (i+batch_size, i)",
        "            positive_mask = torch.zeros_like(mask)",
        "            for i in range(batch_size):",
        "                positive_mask[i, i + batch_size] = True",
        "                positive_mask[i + batch_size, i] = True",
        "            ",
        "            # Compute loss",
        "            exp_sim = torch.exp(sim_matrix)",
        "            exp_sim = exp_sim.masked_fill(torch.eye(2 * batch_size, dtype=torch.bool, device=z.device), 0)",
        "            ",
        "            pos_sim = exp_sim.masked_select(positive_mask).view(2 * batch_size, -1)",
        "            neg_sim = exp_sim.masked_select(~positive_mask).view(2 * batch_size, -1)",
        "            ",
        "            loss = -torch.log(pos_sim.sum(dim=1) / (pos_sim.sum(dim=1) + neg_sim.sum(dim=1) + 1e-8))",
        "            aspect_loss = loss.mean()",
        "            ",
        "            loss_dict[aspect] = aspect_loss",
        "            total_loss += weights[aspect] * aspect_loss",
        "        ",
        "        return total_loss, loss_dict",
        "",
        "# Logo-specific contrastive augmentations",
        "class LogoContrastiveAugmentation:",
        "    \"\"\"Sophisticated augmentation preserving logo semantics\"\"\"",
        "    ",
        "    def __init__(self, image_size=224):",
        "        self.image_size = image_size",
        "        ",
        "        # Strong augmentation for view 1",
        "        self.strong_aug = A.Compose([",
        "            A.Resize(image_size + 32, image_size + 32),",
        "            A.RandomResizedCrop(image_size, image_size, scale=(0.6, 1.0), ratio=(0.8, 1.2)),",
        "            A.HorizontalFlip(p=0.5),",
        "            A.ShiftScaleRotate(shift_limit=0.1, scale_limit=0.2, rotate_limit=15, p=0.7),",
        "            A.OneOf([",
        "                A.ColorJitter(brightness=0.4, contrast=0.4, saturation=0.3, hue=0.1, p=1.0),",
        "                A.RGBShift(r_shift_limit=20, g_shift_limit=20, b_shift_limit=20, p=1.0),",
        "                A.ChannelShuffle(p=1.0)",
        "            ], p=0.8),",
        "            A.OneOf([",
        "                A.GaussianBlur(blur_limit=(3, 7), p=1.0),",
        "                A.MotionBlur(blur_limit=7, p=1.0),",
        "                A.GaussNoise(var_limit=(10, 50), p=1.0)",
        "            ], p=0.3),",
        "            A.CoarseDropout(max_holes=8, max_height=32, max_width=32, ",
        "                          min_holes=1, min_height=8, min_width=8, ",
        "                          fill_value=255, p=0.3),",
        "            A.Normalize(mean=[0.485, 0.456, 0.406], std=[0.229, 0.224, 0.225]),",
        "            ToTensorV2()",
        "        ])",
        "        ",
        "        # Weaker augmentation for view 2",
        "        self.weak_aug = A.Compose([",
        "            A.Resize(image_size + 16, image_size + 16),",
        "            A.RandomResizedCrop(image_size, image_size, scale=(0.8, 1.0), ratio=(0.9, 1.1)),",
        "            A.HorizontalFlip(p=0.5),",
        "            A.ShiftScaleRotate(shift_limit=0.05, scale_limit=0.1, rotate_limit=8, p=0.5),",
        "            A.ColorJitter(brightness=0.2, contrast=0.2, saturation=0.1, hue=0.05, p=0.5),",
        "            A.GaussianBlur(blur_limit=(3, 5), p=0.1),",
        "            A.Normalize(mean=[0.485, 0.456, 0.406], std=[0.229, 0.224, 0.225]),",
        "            ToTensorV2()",
        "        ])",
        "    ",
        "    def __call__(self, image):",
        "        # Convert PIL to numpy if needed",
        "        if isinstance(image, Image.Image):",
        "            image = np.array(image)",
        "        ",
        "        # Generate two augmented views",
        "        view1 = self.strong_aug(image=image)['image']",
        "        view2 = self.weak_aug(image=image)['image']",
        "        ",
        "        return view1, view2",
        "",
        "# Self-supervised pre-training dataset",
        "class LogoContrastiveDataset(Dataset):",
        "    def __init__(self, image_paths, transform=None):",
        "        self.image_paths = image_paths",
        "        self.transform = transform if transform else LogoContrastiveAugmentation()",
        "    ",
        "    def __len__(self):",
        "        return len(self.image_paths)",
        "    ",
        "    def __getitem__(self, idx):",
        "        image_path = self.image_paths[idx]",
        "        image = Image.open(image_path).convert('RGB')",
        "        ",
        "        # Generate contrastive views",
        "        view1, view2 = self.transform(image)",
        "        ",
        "        return view1, view2, idx",
        "",
        "def pretrain_with_simclr(model, dataloader, epochs=100, lr=1e-3, device='cuda'):",
        "    \"\"\"Self-supervised pre-training with SimCLR\"\"\"",
        "    ",
        "    print(\"🔥 Starting SimCLR pre-training...\")",
        "    ",
        "    # Create SimCLR wrapper",
        "    simclr_model = LogoSimCLR(model).to(device)",
        "    ",
        "    # Advanced optimizer",
        "    optimizer = optim.AdamW(simclr_model.parameters(), lr=lr, weight_decay=1e-4)",
        "    scheduler = optim.lr_scheduler.CosineAnnealingWarmRestarts(optimizer, T_0=10, T_mult=2)",
        "    ",
        "    scaler = GradScaler()",
        "    ",
        "    # Training loop",
        "    for epoch in range(epochs):",
        "        simclr_model.train()",
        "        total_loss = 0",
        "        ",
        "        for batch_idx, (view1, view2, _) in enumerate(dataloader):",
        "            view1, view2 = view1.to(device), view2.to(device)",
        "            ",
        "            optimizer.zero_grad()",
        "            ",
        "            with autocast():",
        "                projections = simclr_model(view1, view2)",
        "                loss, loss_dict = simclr_model.contrastive_loss(projections)",
        "            ",
        "            scaler.scale(loss).backward()",
        "            scaler.step(optimizer)",
        "            scaler.update()",
        "            ",
        "            total_loss += loss.item()",
        "            ",
        "            if batch_idx % 20 == 0:",
        "                print(f\"Epoch {epoch}, Batch {batch_idx}: Loss = {loss.item():.4f}\")",
        "                print(f\"  Component losses: {' | '.join([f'{k}: {v.item():.4f}' for k, v in loss_dict.items()])}\")",
        "        ",
        "        scheduler.step()",
        "        avg_loss = total_loss / len(dataloader)",
        "        print(f\"Epoch {epoch}: Average Loss = {avg_loss:.4f}, LR = {scheduler.get_last_lr()[0]:.2e}\")",
        "        ",
        "        # Save checkpoint",
        "        if (epoch + 1) % 10 == 0:",
        "            torch.save({",
        "                'epoch': epoch,",
        "                'model_state_dict': model.state_dict(),",
        "                'optimizer_state_dict': optimizer.state_dict(),",
        "                'loss': avg_loss",
        "            }, f'simclr_checkpoint_epoch_{epoch+1}.pth')",
        "    ",
        "    print(\"✅ SimCLR pre-training completed\")",
