        "# Logo-Aware Vision Transformer with Specialized Attention",
        "class LogoAwareAttention(nn.Module):",
        "    \"\"\"Specialized attention mechanism for logo features\"\"\"",
        "    ",
        "    def __init__(self, dim, heads=8, dim_head=64, dropout=0.1):",
        "        super().__init__()",
        "        inner_dim = dim_head * heads",
        "        project_out = not (heads == 1 and dim_head == dim)",
        "",
        "        self.heads = heads",
        "        self.scale = dim_head ** -0.5",
        "",
        "        self.attend = nn.Softmax(dim=-1)",
        "        self.dropout = nn.Dropout(dropout)",
        "",
        "        self.to_qkv = nn.Linear(dim, inner_dim * 3, bias=False)",
        "        ",
        "        # Logo-specific feature extractors",
        "        self.logo_type_embedding = nn.Parameter(torch.randn(4, dim))  # 4 logo types",
        "        self.geometric_attention = nn.Linear(dim, dim)",
        "        self.color_attention = nn.Linear(dim, dim)",
        "        self.text_attention = nn.Linear(dim, dim)",
        "",
        "        self.to_out = nn.Sequential(",
        "            nn.Linear(inner_dim, dim),",
        "            nn.Dropout(dropout)",
        "        ) if project_out else nn.Identity()",
        "",
        "    def forward(self, x):",
        "        qkv = self.to_qkv(x).chunk(3, dim=-1)",
        "        q, k, v = map(lambda t: rearrange(t, 'b n (h d) -> b h n d', h=self.heads), qkv)",
        "",
        "        # Standard attention",
        "        dots = torch.matmul(q, k.transpose(-1, -2)) * self.scale",
        "        ",
        "        # Logo-aware attention modifications",
        "        # Boost attention for geometric patterns (corners, edges)",
        "        geometric_boost = torch.sigmoid(self.geometric_attention(x.mean(dim=1, keepdim=True)))",
        "        dots = dots + geometric_boost.unsqueeze(1)",
        "        ",
        "        # Enhanced attention for color consistency",
        "        color_consistency = torch.sigmoid(self.color_attention(x.mean(dim=1, keepdim=True)))",
        "        dots = dots * (1 + 0.2 * color_consistency.unsqueeze(1))",
        "        ",
        "        # Text region focus",
        "        text_focus = torch.sigmoid(self.text_attention(x.mean(dim=1, keepdim=True)))",
        "        dots = dots + 0.1 * text_focus.unsqueeze(1)",
        "",
        "        attn = self.attend(dots)",
        "        attn = self.dropout(attn)",
        "",
        "        out = torch.matmul(attn, v)",
        "        out = rearrange(out, 'b h n d -> b n (h d)')",
        "        return self.to_out(out)",
        "",
        "class LogoViTBlock(nn.Module):",
        "    \"\"\"Vision Transformer block with logo-aware attention\"\"\"",
        "    ",
        "    def __init__(self, dim, heads, dim_head, mlp_dim, dropout=0.1):",
        "        super().__init__()",
        "        self.norm1 = nn.LayerNorm(dim)",
        "        self.attn = LogoAwareAttention(dim, heads, dim_head, dropout)",
        "        self.norm2 = nn.LayerNorm(dim)",
        "        ",
        "        # Enhanced MLP with logo-specific processing",
        "        self.mlp = nn.Sequential(",
        "            nn.Linear(dim, mlp_dim),",
        "            nn.GELU(),",
        "            nn.Dropout(dropout),",
        "            nn.Linear(mlp_dim, mlp_dim // 2),",
        "            nn.GELU(),",
        "            nn.Dropout(dropout * 0.5),",
        "            nn.Linear(mlp_dim // 2, dim),",
        "            nn.Dropout(dropout)",
        "        )",
        "        ",
        "        # Residual scaling for better training dynamics",
        "        self.gamma1 = nn.Parameter(torch.ones(dim) * 0.1)",
        "        self.gamma2 = nn.Parameter(torch.ones(dim) * 0.1)",
        "",
        "    def forward(self, x):",
        "        # Pre-norm with learnable scaling",
        "        x = x + self.gamma1 * self.attn(self.norm1(x))",
        "        x = x + self.gamma2 * self.mlp(self.norm2(x))",
        "        return x",
        "",
        "class AdvancedLogoViT(nn.Module):",
        "    \"\"\"State-of-the-art Vision Transformer for logo classification\"\"\"",
        "    ",
        "    def __init__(self, image_size=224, patch_size=16, num_classes=4, dim=768, ",
        "                 depth=12, heads=12, mlp_dim=3072, pool='cls', channels=3,",
        "                 dim_head=64, dropout=0.1, emb_dropout=0.1):",
        "        super().__init__()",
        "        ",
        "        image_height, image_width = (image_size, image_size)",
        "        patch_height, patch_width = (patch_size, patch_size)",
        "",
        "        assert image_height % patch_height == 0 and image_width % patch_width == 0",
        "",
        "        num_patches = (image_height // patch_height) * (image_width // patch_width)",
        "        patch_dim = channels * patch_height * patch_width",
        "        assert pool in {'cls', 'mean'}",
        "",
        "        # Enhanced patch embedding with logo-aware preprocessing",
        "        self.to_patch_embedding = nn.Sequential(",
        "            # Logo-specific convolution layers",
        "            nn.Conv2d(channels, dim // 4, kernel_size=7, stride=2, padding=3),",
        "            nn.BatchNorm2d(dim // 4),",
        "            nn.GELU(),",
        "            nn.Conv2d(dim // 4, dim // 2, kernel_size=3, stride=2, padding=1),",
        "            nn.BatchNorm2d(dim // 2),",
        "            nn.GELU(),",
        "            nn.Conv2d(dim // 2, dim, kernel_size=3, stride=2, padding=1),",
        "            nn.BatchNorm2d(dim),",
        "            Rearrange('b d h w -> b (h w) d'),",
        "        )",
        "        ",
        "        # Recalculate num_patches after convolutions",
        "        conv_output_size = image_size // 8  # Due to 3 conv layers with stride 2",
        "        num_patches = conv_output_size * conv_output_size",
        "",
        "        self.pos_embedding = nn.Parameter(torch.randn(1, num_patches + 1, dim))",
        "        self.cls_token = nn.Parameter(torch.randn(1, 1, dim))",
        "        self.dropout = nn.Dropout(emb_dropout)",
        "",
        "        # Logo-aware transformer blocks",
        "        self.transformer = nn.ModuleList([",
        "            LogoViTBlock(dim, heads, dim_head, mlp_dim, dropout)",
        "            for _ in range(depth)",
        "        ])",
        "",
        "        self.pool = pool",
        "        self.to_latent = nn.Identity()",
        "",
        "        # Advanced classification head with uncertainty estimation",
        "        self.mlp_head = nn.Sequential(",
        "            nn.LayerNorm(dim),",
        "            nn.Linear(dim, mlp_dim),",
        "            nn.GELU(),",
        "            nn.Dropout(dropout),",
        "            nn.Linear(mlp_dim, mlp_dim // 2),",
        "            nn.GELU(),",
        "            nn.Dropout(dropout * 0.5),",
        "            nn.Linear(mlp_dim // 2, num_classes)",
        "        )",
        "        ",
        "        # Uncertainty estimation head",
        "        self.uncertainty_head = nn.Sequential(",
        "            nn.LayerNorm(dim),",
        "            nn.Linear(dim, dim // 2),",
        "            nn.GELU(),",
        "            nn.Linear(dim // 2, 1),",
        "            nn.Sigmoid()",
        "        )",
        "        ",
        "        # Logo type prior network",
        "        self.logo_type_prior = nn.Sequential(",
        "            nn.AdaptiveAvgPool1d(1),",
        "            nn.Flatten(),",
        "            nn.Linear(dim, num_classes),",
        "            nn.Softmax(dim=-1)",
        "        )",
        "",
        "    def forward(self, img, return_attention=False):",
        "        # Enhanced patch embedding",
        "        x = self.to_patch_embedding(img)",
        "        b, n, _ = x.shape",
        "",
        "        cls_tokens = repeat(self.cls_token, '1 1 d -> b 1 d', b=b)",
        "        x = torch.cat((cls_tokens, x), dim=1)",
        "        x += self.pos_embedding[:, :(n + 1)]",
        "        x = self.dropout(x)",
        "",
        "        # Store attention weights for analysis",
        "        attention_weights = []",
        "        ",
        "        # Transform through logo-aware blocks",
        "        for transformer_block in self.transformer:",
        "            x = transformer_block(x)",
        "            if return_attention:",
        "                # Extract attention from the block",
        "                attention_weights.append(transformer_block.attn.attend.weight if hasattr(transformer_block.attn.attend, 'weight') else None)",
        "",
        "        # Pooling and classification",
        "        x = x.mean(dim=1) if self.pool == 'mean' else x[:, 0]",
        "        x = self.to_latent(x)",
        "        ",
        "        # Multi-head outputs",
        "        logits = self.mlp_head(x)",
        "        uncertainty = self.uncertainty_head(x)",
        "        logo_prior = self.logo_type_prior(x.unsqueeze(-1))",
        "        ",
        "        if return_attention:",
        "            return logits, uncertainty, logo_prior, attention_weights",
        "        return logits, uncertainty, logo_prior",
        "",
        "print(\"🎯 Advanced Logo-Aware Vision Transformer implemented\")"
      ]
