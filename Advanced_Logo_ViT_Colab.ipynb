        "    ",
        "    print(f\"Best Backbone: {backbone_names[best_architecture['backbone']['best']]}\")",
        "    print(f\"Best Attention: {attention_names[best_architecture['attention']['best']]}\")",
        "    print(f\"Best Classifier: {classifier_names[best_architecture['classifier']['best']]}\")",
        "    ",
        "    return search_space, best_architecture",
        "",
