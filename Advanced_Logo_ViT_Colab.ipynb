        "# Neural Architecture Search for Optimal Logo Classification Architecture",
        "class LogoNASSpace(nn.Module):",
        "    \"\"\"Searchable architecture space for logo classification\"\"\"",
        "    ",
        "    def __init__(self, num_classes=4):",
        "        super().__init__()",
        "        self.num_classes = num_classes",
        "        ",
        "        # Searchable backbone options",
        "        self.backbone_choices = nn.ModuleList([",
        "            timm.create_model('efficientnet_b0', pretrained=True, num_classes=0),",
        "            timm.create_model('efficientnet_b1', pretrained=True, num_classes=0),",
        "            timm.create_model('mobilenetv3_large_100', pretrained=True, num_classes=0),",
        "            timm.create_model('resnet50d', pretrained=True, num_classes=0),",
        "            AdvancedLogoViT(num_classes=0)  # Our custom ViT",
        "        ])",
        "        ",
        "        # Searchable attention mechanisms",
        "        self.attention_choices = nn.ModuleList([",
        "            nn.Identity(),  # No attention",
        "            nn.MultiheadAttention(embed_dim=512, num_heads=8, batch_first=True),",
        "            self._create_cbam(),  # Convolutional Block Attention Module",
        "            self._create_se_block(),  # Squeeze-and-Excitation",
        "        ])",
        "        ",
        "        # Searchable classifier heads",
        "        self.classifier_choices = nn.ModuleList([",
        "            self._create_simple_head(),",
        "            self._create_mlp_head(),",
        "            self._create_attention_head(),",
        "            self._create_ensemble_head()",
        "        ])",
        "        ",
        "        # Architecture parameters (learnable)",
        "        self.backbone_alpha = nn.Parameter(torch.randn(len(self.backbone_choices)))",
        "        self.attention_alpha = nn.Parameter(torch.randn(len(self.attention_choices)))",
        "        self.classifier_alpha = nn.Parameter(torch.randn(len(self.classifier_choices)))",
        "        ",
        "        # Feature dimension adaptation",
        "        self.feature_adapters = nn.ModuleList([",
        "            nn.Linear(1280, 512),  # EfficientNet-B0",
        "            nn.Linear(1280, 512),  # EfficientNet-B1",
        "            nn.Linear(960, 512),   # MobileNetV3",
        "            nn.Linear(2048, 512),  # ResNet50",
        "            nn.Linear(768, 512)    # ViT",
        "        ])",
        "    ",
        "    def _create_cbam(self):",
        "        \"\"\"Convolutional Block Attention Module\"\"\"",
        "        class CBAM(nn.Module):",
        "            def __init__(self, channels=512, reduction=16):",
        "                super().__init__()",
        "                self.channel_attention = nn.Sequential(",
        "                    nn.AdaptiveAvgPool2d(1),",
        "                    nn.Conv2d(channels, channels // reduction, 1),",
        "                    nn.ReLU(),",
        "                    nn.Conv2d(channels // reduction, channels, 1),",
        "                    nn.Sigmoid()",
        "                )",
        "                self.spatial_attention = nn.Sequential(",
        "                    nn.Conv2d(2, 1, kernel_size=7, padding=3),",
        "                    nn.Sigmoid()",
        "                )",
        "            ",
        "            def forward(self, x):",
        "                # Assume x is [batch, features] - reshape for conv operations",
        "                if x.dim() == 2:",
        "                    x = x.unsqueeze(-1).unsqueeze(-1)  # [batch, features, 1, 1]",
        "                ",
        "                # Channel attention",
        "                ca = self.channel_attention(x)",
        "                x = x * ca",
        "                ",
        "                # Spatial attention (simplified for 1D case)",
        "                avg_pool = torch.mean(x, dim=1, keepdim=True)",
        "                max_pool = torch.max(x, dim=1, keepdim=True)[0]",
        "                spatial_input = torch.cat([avg_pool, max_pool], dim=1)",
        "                sa = self.spatial_attention(spatial_input)",
        "                x = x * sa",
        "                ",
        "                return x.squeeze(-1).squeeze(-1)",
        "        ",
        "        return CBAM()",
        "    ",
        "    def _create_se_block(self):",
        "        \"\"\"Squeeze-and-Excitation block\"\"\"",
        "        return nn.Sequential(",
        "            nn.AdaptiveAvgPool1d(1),",
        "            nn.Flatten(),",
        "            nn.Linear(512, 32),",
        "            nn.ReLU(),",
        "            nn.Linear(32, 512),",
        "            nn.Sigmoid()",
        "        )",
        "    ",
        "    def _create_simple_head(self):",
        "        return nn.Linear(512, self.num_classes)",
        "    ",
        "    def _create_mlp_head(self):",
        "        return nn.Sequential(",
        "            nn.Linear(512, 256),",
        "            nn.ReLU(),",
        "            nn.Dropout(0.3),",
        "            nn.Linear(256, self.num_classes)",
        "        )",
        "    ",
        "    def _create_attention_head(self):",
        "        return nn.Sequential(",
        "            nn.MultiheadAttention(512, 8, batch_first=True),",
        "            nn.Linear(512, self.num_classes)",
        "        )",
        "    ",
        "    def _create_ensemble_head(self):",
        "        return nn.Sequential(",
        "            nn.Linear(512, 256),",
        "            nn.ReLU(),",
        "            nn.Linear(256, 128),",
        "            nn.ReLU(),",
        "            nn.Linear(128, self.num_classes)",
        "        )",
        "    ",
        "    def forward(self, x):",
        "        # Differentiable architecture search",
        "        backbone_weights = F.softmax(self.backbone_alpha, dim=0)",
        "        attention_weights = F.softmax(self.attention_alpha, dim=0)",
        "        classifier_weights = F.softmax(self.classifier_alpha, dim=0)",
        "        ",
        "        # Weighted backbone ensemble",
        "        backbone_outputs = []",
        "        for i, backbone in enumerate(self.backbone_choices):",
        "            if i == 4:  # ViT case",
        "                features, _, _ = backbone(x)  # ViT returns multiple outputs",
        "                features = features.mean(dim=1) if features.dim() > 2 else features",
        "            else:",
        "                features = backbone(x)",
        "                if features.dim() > 2:",
        "                    features = F.adaptive_avg_pool2d(features, 1).flatten(1)",
        "            ",
        "            # Adapt to common feature dimension",
        "            features = self.feature_adapters[i](features)",
        "            backbone_outputs.append(features)",
        "        ",
        "        # Combine backbone outputs",
        "        combined_features = sum(w * feat for w, feat in zip(backbone_weights, backbone_outputs))",
        "        ",
        "        # Apply attention mechanisms",
        "        attention_outputs = []",
        "        for i, attention in enumerate(self.attention_choices):",
        "            if isinstance(attention, nn.MultiheadAttention):",
        "                # MultiheadAttention expects 3D input",
        "                feat_3d = combined_features.unsqueeze(1)  # [batch, 1, features]",
        "                attended, _ = attention(feat_3d, feat_3d, feat_3d)",
        "                attended = attended.squeeze(1)",
        "            else:",
        "                attended = attention(combined_features)",
        "            attention_outputs.append(attended)",
        "        ",
        "        # Combine attention outputs",
        "        final_features = sum(w * feat for w, feat in zip(attention_weights, attention_outputs))",
        "        ",
        "        # Apply classifier heads",
        "        classifier_outputs = []",
        "        for i, classifier in enumerate(self.classifier_choices):",
        "            if i == 2:  # Attention head case",
        "                feat_3d = final_features.unsqueeze(1)",
        "                attended, _ = classifier[0](feat_3d, feat_3d, feat_3d)",
        "                output = classifier[1](attended.squeeze(1))",
        "            else:",
        "                output = classifier(final_features)",
        "            classifier_outputs.append(output)",
        "        ",
        "        # Final weighted combination",
        "        final_output = sum(w * out for w, out in zip(classifier_weights, classifier_outputs))",
        "        ",
        "        return final_output, {",
        "            'backbone_weights': backbone_weights,",
        "            'attention_weights': attention_weights,",
        "            'classifier_weights': classifier_weights",
        "        }",
        "",
        "class LogoNASTrainer:",
        "    \"\"\"Trainer for Neural Architecture Search\"\"\"",
        "    ",
        "    def __init__(self, search_space, train_loader, val_loader, device='cuda'):",
        "        self.search_space = search_space.to(device)",
        "        self.train_loader = train_loader",
        "        self.val_loader = val_loader",
        "        self.device = device",
        "        ",
        "        # Separate optimizers for weights and architecture",
        "        self.weight_optimizer = optim.AdamW(",
        "            [p for n, p in search_space.named_parameters() if 'alpha' not in n],",
        "            lr=1e-3, weight_decay=1e-4",
        "        )",
        "        ",
        "        self.arch_optimizer = optim.Adam(",
        "            [p for n, p in search_space.named_parameters() if 'alpha' in n],",
        "            lr=3e-4",
        "        )",
        "        ",
        "        self.criterion = nn.CrossEntropyLoss()",
        "        ",
        "    def search(self, epochs=50):",
        "        \"\"\"Perform differentiable architecture search\"\"\"",
        "        print(\"🔍 Starting Neural Architecture Search...\")",
        "        ",
        "        best_arch = None",
        "        best_acc = 0",
        "        ",
        "        for epoch in range(epochs):",
        "            # Train weights",
        "            train_loss, train_acc = self._train_weights()",
        "            ",
        "            # Search architecture",
        "            if epoch > 10:  # Start architecture search after weight training",
        "                arch_loss = self._train_architecture()",
        "            ",
        "            # Validate",
        "            val_loss, val_acc = self._validate()",
        "            ",
        "            # Save best architecture",
        "            if val_acc > best_acc:",
        "                best_acc = val_acc",
        "                best_arch = self._get_current_architecture()",
        "            ",
        "            print(f\"Epoch {epoch}: Train Acc = {train_acc:.2f}%, Val Acc = {val_acc:.2f}%\")",
        "            ",
        "            # Print current architecture weights",
        "            if epoch % 10 == 0:",
        "                self._print_architecture_weights()",
        "        ",
        "        print(f\"\🎯 Best architecture found with {best_acc:.2f}% validation accuracy\")",
        "        return best_arch",
        "    ",
        "    def _train_weights(self):",
        "        self.search_space.train()",
        "        total_loss = 0",
        "        correct = 0",
        "        total = 0",
        "        ",
        "        for data, target in self.train_loader:",
        "            data, target = data.to(self.device), target.to(self.device)",
        "            ",
        "            self.weight_optimizer.zero_grad()",
        "            output, _ = self.search_space(data)",
        "            loss = self.criterion(output, target)",
        "            loss.backward()",
        "            self.weight_optimizer.step()",
        "            ",
        "            total_loss += loss.item()",
        "            pred = output.argmax(dim=1)",
        "            correct += (pred == target).sum().item()",
        "            total += target.size(0)",
        "        ",
        "        return total_loss / len(self.train_loader), 100.0 * correct / total",
        "    ",
        "    def _train_architecture(self):",
        "        self.search_space.train()",
        "        ",
        "        # Sample validation batch for architecture optimization",
        "        data, target = next(iter(self.val_loader))",
        "        data, target = data.to(self.device), target.to(self.device)",
        "        ",
        "        self.arch_optimizer.zero_grad()",
        "        output, _ = self.search_space(data)",
        "        loss = self.criterion(output, target)",
        "        loss.backward()",
        "        self.arch_optimizer.step()",
        "        ",
        "        return loss.item()",
        "    ",
        "    def _validate(self):",
        "        self.search_space.eval()",
        "        total_loss = 0",
        "        correct = 0",
        "        total = 0",
        "        ",
        "        with torch.no_grad():",
        "            for data, target in self.val_loader:",
        "                data, target = data.to(self.device), target.to(self.device)",
        "                output, _ = self.search_space(data)",
        "                loss = self.criterion(output, target)",
        "                ",
        "                total_loss += loss.item()",
        "                pred = output.argmax(dim=1)",
        "                correct += (pred == target).sum().item()",
        "                total += target.size(0)",
        "        ",
        "        return total_loss / len(self.val_loader), 100.0 * correct / total",
        "    ",
        "    def _get_current_architecture(self):",
        "        \"\"\"Extract current architecture configuration\"\"\"",
        "        with torch.no_grad():",
        "            backbone_weights = F.softmax(self.search_space.backbone_alpha, dim=0)",
        "            attention_weights = F.softmax(self.search_space.attention_alpha, dim=0)",
        "            classifier_weights = F.softmax(self.search_space.classifier_alpha, dim=0)",
        "            ",
        "            return {",
        "                'backbone': {",
        "                    'weights': backbone_weights.cpu().numpy(),",
        "                    'best': torch.argmax(backbone_weights).item()",
        "                },",
        "                'attention': {",
        "                    'weights': attention_weights.cpu().numpy(),",
        "                    'best': torch.argmax(attention_weights).item()",
        "                },",
        "                'classifier': {",
        "                    'weights': classifier_weights.cpu().numpy(),",
        "                    'best': torch.argmax(classifier_weights).item()",
        "                }",
        "            }",
        "    ",
        "    def _print_architecture_weights(self):",
        "        arch = self._get_current_architecture()",
