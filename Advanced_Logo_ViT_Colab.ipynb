        "# Advanced Sharpness-Aware Minimization for better generalization",
        "class LogoSAM(torch.optim.Optimizer):",
        "    \"\"\"Enhanced SAM optimizer with logo-specific adaptations\"\"\"",
        "    ",
        "    def __init__(self, params, base_optimizer, rho=0.05, adaptive=True, **kwargs):",
        "        assert rho >= 0.0, f\"Invalid rho, should be non-negative: {rho}\"",
        "        ",
        "        defaults = dict(rho=rho, adaptive=adaptive, **kwargs)",
        "        super(LogoSAM, self).__init__(params, defaults)",
        "        ",
        "        self.base_optimizer = base_optimizer(self.param_groups, **kwargs)",
        "        self.param_groups = self.base_optimizer.param_groups",
        "        self.defaults.update(self.base_optimizer.defaults)",
        "        ",
        "        # Logo-specific sharpness tracking",
        "        self.sharpness_history = []",
        "        self.adaptation_factor = 1.0",
        "    ",
        "    @torch.no_grad()",
        "    def first_step(self, zero_grad=False):",
        "        \"\"\"First step: compute gradient and move to worst-case point\"\"\"",
        "        grad_norm = self._grad_norm()",
        "        ",
        "        # Adaptive rho based on logo classification specifics",
        "        effective_rho = self.defaults['rho'] * self.adaptation_factor",
        "        ",
        "        for group in self.param_groups:",
        "            scale = effective_rho / (grad_norm + 1e-12)",
        "            ",
        "            for p in group[\"params\"]:",
        "                if p.grad is None: continue",
        "                ",
        "                # Store original parameters",
        "                self.state[p][\"old_p\"] = p.data.clone()",
        "                ",
        "                # Move to worst-case point",
        "                if group[\"adaptive\"]:",
        "                    e_w = (torch.pow(p, 2) if len(p.shape) > 1 else torch.abs(p)).sqrt().mul_(p.grad).sign()",
        "                else:",
        "                    e_w = p.grad.sign()",
        "                ",
        "                # Logo-specific perturbation scaling",
        "                if 'attention' in str(type(p)) or 'logo' in str(type(p)):",
        "                    e_w *= 0.5  # Smaller perturbations for attention layers",
        "                ",
        "                p.add_(e_w, alpha=scale)",
        "        ",
        "        if zero_grad: self.zero_grad()",
        "    ",
        "    @torch.no_grad()",
        "    def second_step(self, zero_grad=False):",
        "        \"\"\"Second step: compute gradient at worst-case point and update\"\"\"",
        "        for group in self.param_groups:",
        "            for p in group[\"params\"]:",
        "                if p.grad is None: continue",
        "                ",
        "                # Restore original parameters",
        "                p.data = self.state[p][\"old_p\"]",
        "        ",
        "        # Standard optimizer step",
        "        self.base_optimizer.step()",
        "        ",
        "        if zero_grad: self.zero_grad()",
        "    ",
        "    @torch.no_grad()",
        "    def step(self, closure=None):",
        "        \"\"\"Complete SAM step (both first and second steps)\"\"\"",
        "        assert closure is not None, \"SAM requires closure, but it was not provided\"",
        "        ",
        "        # First forward-backward pass",
        "        self.first_step(zero_grad=True)",
        "        ",
        "        # Second forward-backward pass",
        "        loss = closure()",
        "        self.second_step()",
        "        ",
        "        # Update adaptation factor based on loss",
        "        if len(self.sharpness_history) > 10:",
        "            recent_avg = sum(self.sharpness_history[-10:]) / 10",
        "            if loss.item() > recent_avg * 1.1:",
        "                self.adaptation_factor = min(2.0, self.adaptation_factor * 1.05)",
        "            elif loss.item() < recent_avg * 0.9:",
        "                self.adaptation_factor = max(0.5, self.adaptation_factor * 0.95)",
        "        ",
        "        self.sharpness_history.append(loss.item())",
        "        if len(self.sharpness_history) > 100:",
        "            self.sharpness_history.pop(0)",
        "        ",
        "        return loss",
        "    ",
        "    def _grad_norm(self):",
        "        \"\"\"Compute gradient norm across all parameters\"\"\"",
        "        shared_device = self.param_groups[0][\"params\"][0].device",
        "        norm = torch.norm(",
        "            torch.stack([",
        "                ((torch.abs(p) if group[\"adaptive\"] else 1.0) * p.grad).norm().to(shared_device)",
        "                for group in self.param_groups for p in group[\"params\"]",
        "                if p.grad is not None",
        "            ]),",
        "            p=2",
        "        )",
        "        return norm",
        "    ",
        "    def load_state_dict(self, state_dict):",
        "        super().load_state_dict(state_dict)",
        "        self.base_optimizer.param_groups = self.param_groups",
        "",
        "# Multi-optimizer ensemble for different training phases",
        "class AdaptiveOptimizerEnsemble:",
        "    \"\"\"Ensemble of optimizers that adapts based on training phase\"\"\"",
        "    ",
        "    def __init__(self, model, initial_lr=1e-3):",
        "        self.model = model",
        "        self.initial_lr = initial_lr",
        "        ",
        "        # Different optimizers for different phases",
        "        self.optimizers = {",
        "            'warmup': optim.AdamW(model.parameters(), lr=initial_lr/10, weight_decay=1e-4),",
        "            'sam': LogoSAM(model.parameters(), optim.AdamW, rho=0.05, lr=initial_lr, weight_decay=1e-4),",
        "            'ranger': Ranger(model.parameters(), lr=initial_lr, weight_decay=1e-4),",
        "            'fine_tune': optim.SGD(model.parameters(), lr=initial_lr/100, momentum=0.9, weight_decay=1e-4)",
        "        }",
        "        ",
        "        # Schedulers for each optimizer",
        "        self.schedulers = {",
        "            'warmup': optim.lr_scheduler.LinearLR(self.optimizers['warmup'], start_factor=0.1, total_iters=10),",
        "            'sam': optim.lr_scheduler.CosineAnnealingWarmRestarts(self.optimizers['sam'], T_0=20, T_mult=2),",
        "            'ranger': optim.lr_scheduler.OneCycleLR(self.optimizers['ranger'], max_lr=initial_lr, total_steps=100),",
        "            'fine_tune': optim.lr_scheduler.ExponentialLR(self.optimizers['fine_tune'], gamma=0.95)",
        "        }",
        "        ",
        "        self.current_optimizer = 'warmup'",
        "        self.phase_epochs = {'warmup': 10, 'sam': 60, 'ranger': 30, 'fine_tune': 20}",
        "        self.current_epoch = 0",
        "    ",
        "    def get_current_optimizer(self):",
        "        \"\"\"Get optimizer for current training phase\"\"\"",
        "        cumulative_epochs = 0",
        "        for phase, epochs in self.phase_epochs.items():",
        "            cumulative_epochs += epochs",
        "            if self.current_epoch < cumulative_epochs:",
        "                self.current_optimizer = phase",
        "                break",
        "        ",
        "        return self.optimizers[self.current_optimizer]",
        "    ",
        "    def get_current_scheduler(self):",
        "        \"\"\"Get scheduler for current training phase\"\"\"",
        "        return self.schedulers[self.current_optimizer]",
        "    ",
        "    def step(self, loss=None, closure=None):",
        "        \"\"\"Perform optimization step\"\"\"",
        "        optimizer = self.get_current_optimizer()",
        "        scheduler = self.get_current_scheduler()",
        "        ",
        "        if self.current_optimizer == 'sam':",
        "            # SAM requires closure",
        "            if closure is not None:",
        "                loss = optimizer.step(closure)",
        "            else:",
        "                print(\"Warning: SAM optimizer requires closure\")",
        "                return loss",
        "        else:",
        "            optimizer.step()",
        "        ",
        "        scheduler.step()",
        "        return loss",
        "    ",
        "    def zero_grad(self):",
        "        \"\"\"Zero gradients for current optimizer\"\"\"",
        "        self.get_current_optimizer().zero_grad()",
        "    ",
        "    def next_epoch(self):",
        "        \"\"\"Move to next epoch\"\"\"",
        "        self.current_epoch += 1",
        "        print(f\"Epoch {self.current_epoch}: Using {self.current_optimizer} optimizer\")",
        "",
        "print(\"⚡ Advanced SAM optimizer and adaptive ensemble implemented\")"
      ]
